<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :song_artist_name %>
  <%= f.text_field :song_artist_name

  <%= f.label :song_genre_name %>
  <%= f.text_field :song_genre_name %>

  <%= f.submit %>
<% end %>
